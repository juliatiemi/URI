#include <stdio.h>

int main ()


{

	double a;
	double b;
	double c;

	scanf("%lf%lf%lf", &a, &b, &c);

	double a1 = a * c / 2;
	double a2 = 3.14159 * c * c;
	double a3 = ( a + b ) * c / 2;
	double a4 = b * b;
	double a5 = a * b;
	
	printf("TRIANGULO: %.3f\nCIRCULO: %.3f\nTRAPEZIO: %.3f\nQUADRADO: %.3f\nRETANGULO: %.3f\n", a1, a2, a3, a4, a5);

	return 0;

}
